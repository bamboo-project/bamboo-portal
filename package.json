    "ahooks": "^3.3.8",
    "antd": "^4.20.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.1",
    "headlessui": "^0.0.0",
    "js-base64": "^3.7.2",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "react": "17.x",
    "react-dom": "17.x",
    "react-spring": "^9.4.4",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.23",
